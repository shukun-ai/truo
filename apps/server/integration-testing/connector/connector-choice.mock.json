{
  "$schema": "../../../../libs/schema/src/json-schemas/connector.schema.json",
  "label": "test",
  "start": "choice",
  "tasks": {
    "choice": {
      "type": "choice",
      "next": "handleDefaultLanguage",
      "parameters": {
        "conditions": [
          {
            "condition": "$$_js:return $.input.language === 'EN'",
            "next": "handleEnglish"
          },
          {
            "condition": "$$_js:return $.input.language === 'CN'",
            "next": "handleChinese"
          }
        ]
      }
    },
    "handleEnglish": {
      "type": "transformer",
      "parameters": {
        "language": "English"
      }
    },
    "handleChinese": {
      "type": "transformer",
      "parameters": {
        "language": "Chinese"
      }
    },
    "handleDefaultLanguage": {
      "type": "transformer",
      "parameters": {
        "language": "Default"
      }
    }
  }
}
