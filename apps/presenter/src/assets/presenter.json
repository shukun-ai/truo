{
  "label": "test",
  "containers": {
    "jNgZvOaZlgvIXYGXYAFVR": {
      "type": "page",
      "label": "操作台",
      "repositories": {
        "searchForm": {
          "type": "simple",
          "schema": {
            "type": "object",
            "properties": {
              "keyword": { "type": "string" }
            }
          },
          "parameters": {
            "defaultValue": {
              "keyword": "w"
            }
          }
        },
        "airportSearch": {
          "type": "sourceQuery",
          "parameters": {
            "atomName": "airports"
          }
        },
        "dispatchPackage": {
          "type": "flow",
          "parameters": {
            "flowName": "dispatchPackage"
          }
        }
      },
      "widgets": {
        "huYq4i": {
          "tag": "sk-text",
          "label": "欢迎说明",
          "properties": { "value": "欢迎使用操作台" },
          "events": {}
        },
        "FW9ydN": {
          "tag": "sk-form",
          "label": "信息查询",
          "properties": {
            "submitButtonText": "搜索",
            "resetButtonText": "重置",
            "value": "$$_js:return $.searchForm;"
          },
          "events": {
            "submit": [
              {
                "scope": "container",
                "target": "searchForm",
                "action": "setValue"
              }
            ]
          }
        },
        "8hfgZV": {
          "tag": "sk-input",
          "label": "输入关键词",
          "properties": {
            "name": "keyword",
            "placeholder": "",
            "label": "查询关键词"
          },
          "events": {}
        },
        "BpLFZY": {
          "tag": "sk-text",
          "label": "查询内容",
          "properties": {
            "value": "$$_js:return `查询为：${$.searchForm.keyword}`;"
          },
          "events": {}
        },
        "BpLFZX": {
          "tag": "sk-table",
          "label": "表格",
          "properties": {
            "columns": [
              {
                "label": "机场编号",
                "type": "text",
                "field": "code"
              },
              {
                "label": "机场名称",
                "type": "text",
                "field": "title"
              },
              {
                "label": "城市名称",
                "type": "text",
                "field": "cityCode"
              },
              {
                "label": "创建时间",
                "type": "text",
                "field": "createdAt"
              }
            ],
            "value": "$$_js:return $.airportSearch.data.value"
          },
          "events": {}
        }
      },
      "tree": {
        "root": ["huYq4i", "FW9ydN", "BpLFZY", "BpLFZX"],
        "huYq4i": [],
        "FW9ydN": ["8hfgZV"],
        "8hfgZV": [],
        "BpLFZY": [],
        "BpLFZX": []
      },
      "watches": {
        "fetchAirports": {
          "trigger": {
            "stateChanged": [["searchForm", "keyword"]],
            "containerMounted": true,
            "interval": 10000
          },
          "events": [
            {
              "scope": "container",
              "target": "airportSearch",
              "action": "run",
              "value": "$$_js:return { filter: { code: { $like: $.searchForm.keyword } }, limit: 20 };"
            }
          ]
        }
      }
    }
  },
  "screens": {
    "home": {
      "layout": "Dashboard",
      "slots": { "main": "jNgZvOaZlgvIXYGXYAFVR" }
    }
  }
}
