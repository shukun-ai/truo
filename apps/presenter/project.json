{
  "name": "presenter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/presenter/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/presenter",
        "index": "apps/presenter/src/index.html",
        "baseHref": "/",
        "main": "apps/presenter/src/main.ts",
        "polyfills": "apps/presenter/src/polyfills.ts",
        "tsConfig": "apps/presenter/tsconfig.app.json",
        "assets": [
          "apps/presenter/src/favicon.ico",
          "apps/presenter/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/presenter/src/environments/environment.ts",
              "with": "apps/presenter/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "options": {
        "buildTarget": "presenter:build",
        "hmr": true,
        "port": 9010
      },
      "configurations": {
        "production": {
          "buildTarget": "presenter:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "presenter:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/presenter/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/presenter"],
      "options": {
        "jestConfig": "apps/presenter/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
