{
  "$schema": "../../../../schema/src/json-schemas/presenter.schema.json",
  "label": "PDA",
  "widgets": {
    "w1": {
      "tag": "sk-text",
      "label": "text",
      "properties": {
        "value": "{{$.form1.deviceNumber}}"
      },
      "events": {}
    },
    "w2": {
      "tag": "sk-input",
      "label": "deviceNumber",
      "properties": {
        "value": "{{$.form1.deviceNumber}}"
      },
      "events": {
        "value-changed": {
          "process": "w2OnValueChanged",
          "value": ""
        }
      }
    },
    "w3": {
      "tag": "sk-code",
      "label": "code",
      "properties": {
        "value": "{{$.form1}}"
      },
      "events": {}
    }
  },
  "nodes": {
    "root": ["w1", "w2", "w3"]
  },
  "processes": {
    "w2OnValueChanged": {
      "label": "w2OnValueChanged",
      "start": "setValue",
      "tasks": {
        "setValue": {
          "label": "setValue",
          "type": "assign",
          "parameters": {
            "variable": "test",
            "expression": "test"
          }
        }
      }
    }
  },
  "repositories": {
    "form1": {
      "type": "Simple",
      "parameters": {}
    }
  }
}
