{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "PresenterScreen",
  "type": "object",
  "required": ["layout", "slots"],
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "layout": { "type": "string" },
    "slots": {
      "type": "object",
      "patternProperties": {
        "^(.)+$": {
          "type": "string"
        }
      }
    },
    "description": { "type": "string" }
  }
}
