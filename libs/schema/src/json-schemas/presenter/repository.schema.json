{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "PresenterRepository",
  "type": "object",
  "required": ["type", "parameters"],
  "additionalProperties": false,
  "properties": {
    "type": { "type": "string" },
    "parameters": {
      "type": "object",
      "patternProperties": {
        "^(.)+$": {
          "type": ["string", "array", "object", "number", "boolean"]
        }
      }
    }
  }
}
