      "const": "Pass"
    },
    "comment": {
      "type": "string"
    },
    "end": {
      "type": "boolean"
    },
    "next": {
      "type": "string"
    },
    "inputPath": {
      "type": "string"
    },
    "parameters": {
      "type": "object"
    },
    "resultSelector": {
      "type": "object"
    },
    "resultPath": {
      "type": "string"
    },
    "outputPath": {
      "type": "string"
    },
    "result": {
      "type": "object"
    },
    "x": {
      "type": "number"
    },
    "y": {
      "type": "number"
    }
  }
