{
  "$schema": "../../../../../../schema/src/json-schemas/presenter/container.schema.json",
  "type": "page",
  "repositories": {
    "form1": {
      "type": "Simple"
    }
  },
  "widgets": {
    "w1": {
      "tag": "sk-text",
      "properties": {
        "value": "{{$.form1.deviceNumber}}"
      },
      "events": {}
    },
    "w2": {
      "tag": "sk-input",
      "properties": {
        "value": "{{$.form1.deviceNumber}}"
      },
      "events": {
        "value-changed": [
          {
            "action": "setRepository",
            "target": "form1",
            "path": ["deviceNumber"]
          }
        ]
      }
    },
    "w3": {
      "tag": "sk-code",
      "properties": {
        "value": "{{$.form1}}"
      },
      "events": {}
    }
  },
  "root": ["w1", "w2", "w3"],
  "tree": {}
}
