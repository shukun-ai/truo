{
  "$schema": "../../../../../../schema/src/json-schemas/presenter/container.schema.json",
  "type": "page",
  "repositories": {
    "form1": {
      "type": "Simple",
      "parameters": {}
    }
  },
  "widgets": {
    "w1": {
      "tag": "sk-text",
      "label": "text",
      "properties": {
        "value": "{{$.form1.deviceNumber}}"
      },
      "events": {}
    },
    "w2": {
      "tag": "sk-input",
      "label": "deviceNumber",
      "properties": {
        "value": "{{$.form1.deviceNumber}}"
      },
      "events": {
        "value-changed": [
          {
            "scope": "container",
            "target": "form1",
            "action": "setValue",
            "path": ["deviceNumber"]
          }
        ]
      }
    },
    "w3": {
      "tag": "sk-code",
      "label": "code",
      "properties": {
        "value": "{{$.form1}}"
      },
      "events": {}
    }
  },
  "watches": {},
  "tree": {
    "root": ["w1", "w2", "w3"]
  }
}
