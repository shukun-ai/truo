{
  "name": "widget",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/widget/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/widget/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/widget"],
      "options": {
        "jestConfig": "libs/widget/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx nx workspace-generator widget-generator widget"
          }
        ]
      }
    }
  },
  "tags": []
}
